{
  "extends": [
    "airbnb",
    "eslint:recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "import/prefer-default-export": "off",
    "react/destructuring-assignment": "off",
    "react/prop-types": "off",
    "no-console": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "jsx-a11y/click-events-have-key-events": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "semi": [
      "error",
      "always"
    ]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "CHINA_SERVICE_API_BASEURL": false
  }
}
